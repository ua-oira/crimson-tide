{
  "name": "crimson-tide",
  "version": "0.3.11",
  "description": "Reusable React components for The University of Alabama üêò",
  "main": "dist/index.js",
  "author": "Coston Perkins <coston.perkins@ua.edu> (https://coston.io)",
  "homepage": "https://github.com/ua-oira/crimson-tide",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ua-oira/crimson-tide.git"
  },
  "bugs": {
    "url": "https://github.com/ua-oira/crimson-tide/issues"
  },
  "license": "GNU GENERAL PUBLIC LICENSE",
  "engines": {
    "node": ">= 0.10.0"
  },
  "scripts": {
    "build": "babel -d dist src --copy-files",
    "clean": "rm -rf dist && mkdir dist",
    "watch": "babel -w -d dist src",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "test": "jest",
    "preversion": "npm install && npm run clean && npm run format && npm run build && npm run build-storybook",
    "postpublish": "git push && git push --tag",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "collectCoverage": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-info": "^3.4.10",
    "@storybook/addon-links": "^3.4.10",
    "@storybook/addons": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "core-js": "^2.5.7",
    "coveralls": "^3.0.2",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "enzyme-to-json": "^3.3.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "prettier": "^1.14.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "react-shallow-output": "^0.2.0",
    "react-test-renderer": "^16.4.2",
    "storybook-react-router": "^1.0.1"
  },
  "keywords": [
    "react",
    "react-component"
  ],
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "d3-array": "^1.2.4",
    "d3-collection": "^1.0.7",
    "d3-scale": "^2.1.2",
    "gatsby-link": "^1.6.46",
    "immutable": "^3.8.2",
    "react-headroom": "^2.2.2",
    "react-helmet": "^5.2.0",
    "react-obfuscate": "^2.1.7",
    "recompose": "^0.30.0",
    "styled-components": "^3.4.5"
  }
}
